{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "global-require" : "off",
        "no-underscore-dangle" : "off",
        "no-plusplus" : "off",
        "max-len" : "off",
        "no-console" : "off",
        "no-await-in-loop": "off"
    }
}
